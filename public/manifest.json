{
	"manifest_version": 3,
	"name": "Make Youtube Great Again",
	"version": "1.1",
	"description": "Set comfortable number of videos and shorts in a row on a YouTube index page",
	"action": {
		"default_popup": "index.html",
		"default_icon": {
			"16": "vite.png",
			"48": "vite.png",
			"128": "vite.png"
		}
	},
	"permissions": [
		"activeTab",
		"scripting"
	],
	"icons": {
		"16": "vite.png",
		"48": "vite.png",
		"128": "vite.png"
	}
}